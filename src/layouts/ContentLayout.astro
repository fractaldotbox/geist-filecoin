---
import { ContentHeader } from "@/components/react/ContentHeader";
import Layout from "./Layout.astro";

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<Layout title={title}>
    <div class="container mx-auto p-6">
        <ContentHeader client:load title={title} />
        <slot />
    </div>
</Layout> 